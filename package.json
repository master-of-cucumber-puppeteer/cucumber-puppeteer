  "bugs": {
    "url": "https://github.com/master-of-cucumber-puppeteer/cucumber-puppeteer/issues"
  },
  "homepage": "https://github.com/master-of-cucumber-puppeteer/cucumber-puppeteer#readme",
  "dependencies": {
    "cucumber": "^3.1.0",
    "cucumber-html-reporter": "^3.0.4",
    "js-yaml": "^3.10.0",
    "puppeteer": "^0.12.0",
    "uuid": "^3.1.0"
  }
